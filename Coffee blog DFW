	    <p id="quiz">Bonus question</p>
	    
	    
			<p> How many cups of coffee have I had today?</p>
			
		
			
			<input type="text" id="magic-word">
			
			<button id="check">Guess</button>
			
				<p id="guess" class="correctAnswer" class="wrongAnswer"></p>
			
			
				<script type ="text/javascript">
	    
	    	document.getElementById
					("check").onclick = function() {
				
					var magicWordEntered = document.getElementById
					("magic-word").value;
					
					var cupsOfCoffee = 2
					
					if  (magicWordEntered == cupsOfCoffee)  {
						
						document.getElementById("guess").innerHTML =
						"You got it!";
						
					
					} else {
					
					
					document.getElementById("guess").innerHTML =
						"Nope, guess again";
					
					}
					
				
				}
	    
	    
	    
	</script>

